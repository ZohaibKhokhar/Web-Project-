@model List<Customer>
@{
    Layout = "_AdminLayout";
}


<div class="text-center" style="margin-top:80px;">
    <p class="welcome"> Customers</p>
</div>

<div class="container-fluid" style="margin-top:30px;">
    <table class="table table-responsive table-striped table-bordered" style="width:100%">
        <tbody>
            <tr>
                <th><h4>Name</h4></th>
                <th><h4>Email</h4></th>
                <th><h4>Customer Id</h4></th>
                <th><h4>Address</h4></th>
            </tr>

            @foreach (var c in Model)
            {
                <tr>
                    <td>@c.Name</td>
                    <td>@c.Email</td>
                    <td>@c.CustomerId</td>

                    <td>@c.Address</td>
                                       
                </tr>
            }
        </tbody>
    </table>
</div>