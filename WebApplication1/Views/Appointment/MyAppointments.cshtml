@model List<Appointment>

@{
	Layout = "Layout2";
}


<div class="container" style="margin-top:80px;">
    <div class="row">
        <div class="col-lg-4 col-md-3 col-sm-2"></div>

        <div class="col-lg-6 col-md-7 col-sm-8">
            <p class="welcome">Your Appointments</p>

        </div>

        <div class="col-lg-2 col-md-2 col-sm-2"></div>

    </div>

</div>

<div class="container-fluid" style="margin-top:90px; margin-bottom:150px;">
    <table class="table table-responsive table-striped table-bordered" style="width:100%">
        <tbody>
        
                <tr>
                    <th><h5>ID</h5></th>
                    <th><h5>Name</h5></th>
                    <th><h5>Phone number</h5></th>
                    <th><h5>Pet Type</h5></th>
                    <th><h5>Preferred Date and Time</h5></th>
                    <th><h5>Reason</h5></th>
                    <th><h5>Email</h5></th>

                </tr>
       

            @foreach (var app in Model)
            {



                <tbody>
                    <tr>
                        <td>@app.Id</td>
                        <td>@app.Name</td>
                        <td>@app.Phone</td>
                        <td>@app.PetType</td>
                        <td>@app.preferredDateTime</td>
                        <td>@app.Reason</td>
                        <td>@app.Email</td>
                    </tr>
                </tbody>


            }
        </table>

    </div>

</div>